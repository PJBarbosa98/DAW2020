extends layout

block content
	h1 Students List (DAW2020)

	.w3-card-4
		header.w3-container.w3-teal
			h3.w3-left Students List (DAW2020)
		if list.length > 0
			.w3-container
				table.w3-table-all
					tr
						th Number
						th Name
						th Git
					each s in list
						tr
							td= s.numero
							td= s.nome
							td= s.git
		else
			.w3-container
				p There are no students...

		.w3-container.w3-teal
			address Gerado por DAW_APP :: 2020-Nov